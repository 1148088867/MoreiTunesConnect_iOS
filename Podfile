def deful_pods
    inhibit_all_warnings!
    
    # 自动布局
    pod 'Masonry'
    
    # sql数据库
    pod 'YTKKeyValueStore'
    
    # 扩展
    pod 'YYCategories'
    
    # 数据转模型
    pod 'YYModel'
    
    # 网络
    pod 'DWNetworking'
    pod 'YYWebImage'
    
end

target 'MoreiTunesConnect_iOS' do
    
    # 通用
    deful_pods
    
    # 视图
    pod 'QMUIKit'
    pod 'MJRefresh'
    pod 'IQKeyboardManager'
    pod 'ISMessages'
	pod 'KSPhotoBrowser'
    
    # FPS检测
    pod 'JPFPSStatus'
    
    #pod 'JPush'
    # 扩展
    pod 'DWCategoryKit'
    
end

target 'MoreiTunesConnect_iOS_Extension' do
    inherit! :search_paths
    
    deful_pods
end



